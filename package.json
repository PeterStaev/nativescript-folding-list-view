{
  "name": "nativescript-folding-list-view",
  "version": "1.0.1",
  "description": "A NativeScript ListView widget with foldable cells.",
  "main": "folding-list-view",
  "typings": "folding-list-view.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "4.0.0",
      "android": "4.0.0"
    },
    "plugin": {
      "nan": "false",
      "pan": "true",
      "core3": "true",
      "category": "Interface"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/nativescript-folding-list-view"
  },
  "scripts": {
    "tslint": "tslint --project tsconfig.json",
    "tsc": "tsc --project tsconfig.json",
    "demo-ios": "grunt build && cd demo && tns run ios",
    "demo-android": "grunt build && cd demo && tns run android"
  },
  "keywords": [
    "NativeScript",
    "ListView",
    "FoldingListView",
    "FoldingCell",
    "FoldableCell",
    "tangra",
    "ramotion"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "license": "Apache-2.0",
  "peerDependencies": {
    "tns-core-modules": "^4"
  },
  "devDependencies": {
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "tns-core-modules": "^4.0.1",
    "tns-platform-declarations": "^4.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.7.2"
  }
}
